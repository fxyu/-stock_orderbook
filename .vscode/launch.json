    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "connect": {
              "host": "localhost",
              "port": 5678
            }
          }
        {
            "name": "Python: Futu Main",
            "type": "python",
            "request": "launch",
            "program": "main_futu.py",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "args" : [
                    // "--name", "bwv12" ,
                    // "--resume_all",
                    // "--eval"
                ],
            "env": {
                "PYTHONPATH":"${PYTHONPATH}"
            }
        },
        {
            "name": "Python: Server",
            "type": "python",
            "request": "launch",
            "program": "app.py",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "args" : [

                ],
            "gevent": true,    
            "cwd": "/Users/felix/Project/stock_orderbook/server",
            "env": {
                "PYTHONPATH":"${PYTHONPATH}",
                "FLASK_APP": "server/app.py"
            }
        },
        {
            "name": "Python: debug file",
            "type": "python",
            "request": "launch",
            "program": "debug.py",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "args" : [
                    // "--name", "bwv12" ,
